//O(n), O(n)
class Solution {
public:
    bool isAnagram(string s, string t) {
        if (s.size() != t.size()) return false;
        unordered_map<char, int>smap;//map<char, int>smap is acceptable
        unordered_map<char, int>tmap;
        for (int i = 0; i<s.size(); i++){
            smap[s[i]] += 1;
            tmap[t[i]] += 1;
        }
        return smap == tmap;
    }
};
